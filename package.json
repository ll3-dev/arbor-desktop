    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/ui": "^3.2.4",
    "drizzle-kit": "^0.31.4",
    "electron": "^37.3.0",
    "electron-builder": "^25.1.8",
    "electron-vite": "^4.0.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react": "^7.37.5",
