    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-slot": "^1.2.3",
    "@tailwindcss/vite": "^4.1.12",
    "@tanstack/react-query": "^5.85.3",
    "@tanstack/react-router": "^1.131.26",
    "@toss/react": "^1.8.1",
    "@trpc/client": "^11.4.4",
    "@trpc/server": "^11.4.4",
    "@trpc/tanstack-react-query": "^11.4.4",
    "class-variance-authority": "^0.7.1",
